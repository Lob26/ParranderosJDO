rootLogger.level = info
rootLogger.appenderRef.datanucleus.ref = datanucleus
rootLogger.appenderRef.parranderos.ref = parranderos
appenders = datanucleus, parranderos

appender.datanucleus.type = File
appender.datanucleus.filename = datanucleus.log
appender.datanucleus.name = datanucleus
appender.datanucleus.append = true
appender.datanucleus.layout.type = PatternLayout
appender.datanucleus.layout.pattern = %d{HH:mm:ss,SSS} (%t) %-5p [%c] - %m%n

appender.parranderos.type = File
appender.parranderos.filename = parranderos.log
appender.parranderos.name = parranderos
appender.parranderos.append = true
appender.parranderos.layout.type = PatternLayout
appender.parranderos.layout.pattern = %d{HH:mm:ss,SSS} (%t) %-5p [%c] - %m%n

property.filename = trace.log

logger.parranderos.name = parranderos_logger
logger.parranderos.level = debug
